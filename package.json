{
	"name": "uni-preset-vue",
	"version": "0.0.0",
	"scripts": {
		"dev:mp-alipay": "uni -p mp-alipay",
		"build:mp-alipay": "uni build -p mp-alipay"
	},
	"uni-app": {
		"scripts": {
			"dev": {
				"title": "开发版",
				"env": {
					"UNI_PLATFORM": "mp-alipay"
				}
			},
			"pre": {
				"title": "测试版",
				"env": {
					"UNI_PLATFORM": "mp-alipay"
				},
				"define": {
					"ALIPAY-TEST": true
				}
			},
			"prod": {
				"title": "正式版",
				"env": {
					"UNI_PLATFORM": "mp-alipay"
				},
				"define": {
					"ALIPAY-PRO": true
				}
			}
		}
	},
	"dependencies": {
		"@dcloudio/uni-app": "3.0.0-3080720230703001",
		"@dcloudio/uni-app-plus": "3.0.0-3080720230703001",
